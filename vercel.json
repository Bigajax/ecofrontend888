{
  "env": {
    "VITE_API_URL": "https://ecobackend888.onrender.com"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://ecofrontend888.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,PATCH,DELETE,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Authorization,Content-Type,X-Requested-With,X-Eco-Guest-Id,X-Guest-Id,X-Client-Id"
        },
        {
          "key": "Access-Control-Expose-Headers",
          "value": "*,X-Eco-Guest-Id,X-Guest-Id"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://ecobackend888.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
